    "express": "^4.16.3",
    "fs": "0.0.1-security",
    "jquery": "^3.3.1",
    "oauth": "^0.9.15",
    "request": "^2.87.0",
    "require": "^2.4.20",
    "trello-web": "^1.0.2",
    "uglify-js": "^3.4.9",
    "url": "^0.11.0"
  }
}
